<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Activivty.RegisterVerhicleInfomation">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:background="#009688"
        app:titleTextColor="#fff"
        android:id="@+id/toolbarVehicleInfomation"
        app:title="Vehicle Infomation"
        android:layout_height="wrap_content"/>
    <ImageView
        android:src="@drawable/xeoto"
        android:scaleType="fitXY"
        android:layout_width="match_parent"
        android:layout_height="200dp"/>
    <TextView
        android:layout_width="match_parent"
        android:text="set up your "
        android:gravity="center"
        android:textSize="20dp"
        android:padding="5dp"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:padding="2dp"
        android:layout_gravity="center"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="#000"
            android:id="@+id/img1"
            android:layout_marginLeft="10dp"
            android:padding="5dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_toRightOf="@+id/img1"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textColor="#000"
            android:textSize="16sp"
            android:text="Ô tô"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_width="30dp"
            android:src="@drawable/icondropdow"
            android:layout_alignParentRight="true"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#6F000000"
            android:layout_toRightOf="@+id/img1"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:padding="2dp"
        android:layout_gravity="center"
        android:layout_marginBottom="10dp"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="#000"
            android:id="@+id/img2"
            android:layout_marginLeft="10dp"
            android:padding="5dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_toRightOf="@+id/img2"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textColor="#000"
            android:textSize="16sp"
            android:id="@+id/txtProducer"
            android:hint="producer"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_width="30dp"
            android:src="@drawable/icondropdow"
            android:layout_alignParentRight="true"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:id="@+id/imgProducer"
            android:scaleType="fitXY"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#6F000000"
            android:layout_toRightOf="@+id/img2"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:padding="2dp"
        android:layout_gravity="center"
        android:layout_marginBottom="10dp"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="#000"
            android:id="@+id/img3"
            android:layout_marginLeft="10dp"
            android:padding="5dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_toRightOf="@+id/img3"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textColor="#000"
            android:textSize="16sp"
            android:hint="Type"
            android:id="@+id/txtType"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_width="30dp"
            android:src="@drawable/icondropdow"
            android:layout_alignParentRight="true"
            android:layout_height="30dp"
            android:id="@+id/imgType"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#6F000000"
            android:layout_toRightOf="@+id/img3"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:padding="2dp"
        android:layout_gravity="center"
        android:layout_marginBottom="10dp"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="#000"
            android:id="@+id/img5"
            android:layout_marginLeft="10dp"
            android:padding="5dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_toRightOf="@+id/img5"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textColor="#000"
            android:textSize="16sp"
            android:hint="Capacity"
            android:id="@+id/txtCapacity"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_width="30dp"
            android:src="@drawable/icondropdow"
            android:layout_alignParentRight="true"
            android:layout_height="30dp"
            android:id="@+id/imgCapacity"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#6F000000"
            android:layout_toRightOf="@+id/img5"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:padding="2dp"
        android:layout_gravity="center"
        android:layout_marginBottom="10dp"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="#000"
            android:id="@+id/img4"
            android:layout_marginLeft="10dp"
            android:padding="5dp"/>
        <EditText
            android:layout_width="match_parent"
            android:layout_toRightOf="@+id/img4"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textColor="#000"
            android:textSize="16sp"
            android:background="@null"
            android:id="@+id/edtNumberContermetPresent"
            android:inputType="number"
            android:hint="number contermet present"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#6F000000"
            android:layout_toRightOf="@+id/img4"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <Button
        android:layout_width="200dp"
        android:text="Register"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:background="#009688"
        android:textColor="#fff"
        android:id="@+id/btnRegister"
        android:textSize="18dp"
        android:layout_height="50dp"/>
    </RelativeLayout>


</LinearLayout>